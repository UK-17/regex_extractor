{
   "DELIMITERS":{
      "SECTION":"\\n{2,}",
      "LINE":"\\n+"
   },
   "MEDICINE_SECTION":"\\bTab\\s*\\.*|\\bSyp\\s*\\.*|\\bInj\\s*\\.*|\\bCap\\s*\\.*"
   ,
    "MRN":{
       "SEARCH_KEY":"(?P<KEY>mrn|REG\\s*\\/\\s*IP\\s*NO|Hospital\\s*[\\s*\\-]?\\s*ID)",
       "SEARCH_DELIMITER":"\\s*(No|num|number)?\\s*\\n*:*",
       "SEARCH_PATTERN":"\\s*\\n*(?P<MRN>[0-9]{2}\\-[0-9]{2}\\-[0-9]{8}|(\\d{7}\\s*/\\s*\\d{6}))"
    },
    "DATE_OF_DISCHARGE":{
       "SEARCH_KEY":"(?P<KEY>Date\\s*of\\s*Discharge|Discharge\\s*Date|Discharged\\s*On)",
       "SEARCH_DELIMITER":"\\s*:*",
       "SEARCH_PATTERN":"\\s*\\n*(?P<DATE_OF_DISCHARGE>([0-9]{1,2})\\s*[\\/\\-]\\s*([a-z]{3,9}|[0-9]{1,2})\\s*[\\/\\-]\\s*([0-9]{2,4}))"
    },
    "DOB":{
       "SEARCH_KEY":"(?P<KEY>Date\\s*of\\s*Birth)",
       "SEARCH_DELIMITER":"\\s*:*",
       "SEARCH_PATTERN":"\\s*\\n*(?P<DOB>([0-9]{1,2})\\s*-([A-Za-z]{3,9}|[0-9]{1,2})\\s*-([0-9]{2,4}))"
    },
    "AGE":{
       "SEARCH_KEY":"(?P<KEY>Age)",
       "SEARCH_DELIMITER":"\\s*:\\s*",
       "SEARCH_PATTERN":"\\s*(?P<AGE>([0-9]{2})\\s*|\\s*([0-9]{2})\\s*(years)|\\s*([0-9]{2})\\s*(y|years)?\\s*([0-9]{1,2})\\s*(m)\\s*([0-9]{1,3})\\s*(d)|Age\\s*:*\\s*(\\d+))"
    },
    "PATIENT_NAME":{
       "SEARCH_KEY":"(?P<KEY>Patient\\s*Name\\s*\\n*|Name\\s*\\n*)",
       "SEARCH_DELIMITER":"\\s*:*(\\s*|\\n*)",
       "SEARCH_PATTERN":"(Mr\\.{0,1}|Mrs\\.{0,1})?(?P<PATIENT_NAME>\\s*\\w*\\s{1,2}\\w*)"
    },
    "DIAGNOSIS":{
       "SEARCH_KEY":"(DIAGNOSIS\\s*)",
       "SEARCH_DELIMITER":"\\s*:*",
       "SEARCH_PATTERN":"(?P<DIAGNOSIS>.*?)\\.*\\n*(?=REASON|CO-MORBIDITIES|CLINICAL\\s*HISTORY|HISTORY\\s*AND\\s*PHYSICAL\\s*FINDINGS|\\s*SURGERY|PROCEDURE\\s*DETAILS|HISTORY)"
    },
    "GENDER":{
       "SEARCH_KEY":"(?P<KEY>Gender|Sex)",
       "SEARCH_DELIMITER":"\\s*:*",
       "SEARCH_PATTERN":"(\\s*|\\n)(?P<GENDER>\\w*)"
    },
    "MEDICINE":"(\\b(?P<FORM>Tab\\s*\\.*|Syp\\s*\\.*|Inj\\s*\\.*|Cap\\s*\\.*)\\s*(?P<NAME>\\w+\\s*\\D*\\s*\\D*)\\s*(?P<DOSAGE>(\\d*(.\\d+)?)\\s*(mg|ml|unit)\\s+)?\\s*(?P<FREQUENCY>\\S*-\\S*-\\S*(-\\d*)?)?\\s*(?P<DURATION>x?\\s*(\\d+)\\s*days|x?\\s*continue|x?\\s*to\\s*continue)?)"
 }